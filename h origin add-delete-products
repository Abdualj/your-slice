[32m+[m[32m        const cancelButtons = document.querySelectorAll('.btn-cancel');[m
[32m+[m[41m        [m
[32m+[m[32m        processButtons.forEach(button => {[m
[32m+[m[32m            button.addEventListener('click', function() {[m
[32m+[m[32m                const statusCell = this.parentElement.previousElementSibling;[m
