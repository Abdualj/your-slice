[32m+[m[32m        const emailInput = document.getElementById('email');[m
[32m+[m[32m        const passwordInput = document.getElementById('password');[m
[32m+[m[32m        const nameError = document.getElementById('nameError');[m
[32m+[m[32m        const emailError = document.getElementById('emailError');[m
[32m+[m[32m        const passwordError = document.getElementById('passwordError');[m
[32m+[m[41m        [m
[32m+[m[32m        // Validate inputs on change[m
[32m+[m[32m        nameInput.addEventListener('input', function() {[m
