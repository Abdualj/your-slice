[32m+[m[32m                nameError.style.display = 'none';[m
[32m+[m[32m            } else {[m
[32m+[m[32m                nameError.style.display = 'block';[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[41m        [m
[32m+[m[32m        emailInput.addEventListener('input', function() {[m
[32m+[m[32m            if (emailInput.validity.valid) {[m
