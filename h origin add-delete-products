[32m+[m[32m                statusCell.innerHTML = '<span class="status status-completed">Completed</span>';[m
[32m+[m[32m                updateStats();[m
[32m+[m[32m            });[m
[32m+[m[32m        });[m
[32m+[m[41m        [m
[32m+[m[32m        cancelButtons.forEach(button => {[m
[32m+[m[32m            button.addEventListener('click', function() {[m
[32m+[m[32m                const statusCell = this.parentElement.previousElementSibling;[m
[32m+[m[32m                statusCell.innerHTML = '<span class="status status-cancelled">Cancelled</span>';[m
[32m+[m[32m                updateStats();[m
[32m+[m[32m            });[m
[32m+[m[32m        });[m
[32m+[m[41m        [m
[32m+[m[32m        // Filter buttons[m
[32m+[m[32m        const filterButtons = document.querySelectorAll('.filter-btn');[m
[32m+[m[32m        filterButtons.forEach(button => {[m
[32m+[m[32m            button.addEventListener('click', function() {[m
[32m+[m[32m                filterButtons.forEach(btn => btn.classList.remove('active'));[m
[32m+[m[32m                this.classList.add('active');[m
[32m+[m[32m                // In a real app, this would filter the orders table[m
[32m+[m[32m            });[m
[32m+[m[32m        });[m
[32m+[m[41m        [m
[32m+[m[32m        function updateStats() {[m
[32m+[m[32m            // In a real app, this would update the dashboard stats[m
[32m+[m[32m            console.log('Stats would update here');[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
