[32m+[m[32m            } else {[m
[32m+[m[32m                emailError.style.display = 'block';[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[41m        [m
[32m+[m[32m        passwordInput.addEventListener('input', function() {[m
[32m+[m[32m            if (passwordInput.validity.valid) {[m
[32m+[m[32m                passwordError.style.display = 'none';[m
[32m+[m[32m            } else {[m
[32m+[m[32m                passwordError.style.display = 'block';[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[41m        [m
[32m+[m[32m        // Handle form submission[m
[32m+[m[32m        loginForm.addEventListener('submit', function(e) {[m
[32m+[m[32m            e.preventDefault();[m
[32m+[m[41m            [m
[32m+[m[32m            let isValid = true;[m
[32m+[m[41m            [m
[32m+[m[32m            // Validate name[m
[32m+[m[32m            if (nameInput.value.length < 3) {[m
[32m+[m[32m                nameError.style.display = 'block';[m
[32m+[m[32m                isValid = false;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                nameError.style.display = 'none';[m
[32m+[m[32m            }[m
[32m+[m[41m            [m
[32m+[m[32m            // Validate email[m
[32m+[m[32m            if (!emailInput.value.endsWith('@metropolia.fi')) {[m
[32m+[m[32m                emailError.style.display = 'block';[m
[32m+[m[32m                isValid = false;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                emailError.style.display = 'none';[m
[32m+[m[32m            }[m
[32m+[m[41m            [m
[32m+[m[32m            // Validate password[m
[32m+[m[32m            const hasUpperCase = /[A-Z]/.test(passwordInput.value);[m
[32m+[m[32m            const hasNumber = /[0-9]/.test(passwordInput.value);[m
[32m+[m[41m            [m
[32m+[m[32m            if (passwordInput.value.length < 6 || !hasUpperCase || !hasNumber) {[m
[32m+[m[32m                passwordError.style.display = 'block';[m
[32m+[m[32m                isValid = false;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                passwordError.style.display = 'none';[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (isValid) {[m
[32m+[m[32m                dialog.close();[m
[32m+[m[32m                document.querySelector('.admin-container').style.display = 'flex';[m
[32m+[m[32m                document.body.classList.remove('pre-login');[m
[32m+[m[32m                document.body.classList.add('logged-in');[m
[32m+[m[32m            }[m
[32m+[m[32m        }); // This closing bracket was missing[m
[32m+[m[41m        [m
[32m+[m[32m        // Cancel button functionality[m
[32m+[m[32m        const cancelBtn = document.querySelector('.cancel-btn');[m
[32m+[m[32m        cancelBtn.addEventListener('click', function() {[m
[32m+[m[32m            dialog.close();[m
[32m+[m[32m        });[m
[32m+[m[41m        [m
[32m+[m[32m        // Original dashboard functionality[m
